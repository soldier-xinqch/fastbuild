<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fastbuild.mapper.AuthRoleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.fastbuild.entity.AuthRole">
        <id column="id" property="id" />
        <result column="role_type" property="roleType" />
        <result column="role_name" property="roleName" />
        <result column="role_auth" property="roleAuth" />
        <result column="status" property="status" />
        <result column="bak" property="bak" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, role_type AS roleType, role_name AS roleName, role_auth AS roleAuth, status, bak
    </sql>

</mapper>
